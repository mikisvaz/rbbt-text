rbbt-text=/$PWD filter="*.rb *.rake Rakefile *.rdoc *.R *.sh *.js *.haml *.sass *.txt *.conf" {
 Rakefile
 bin=bin {
  get_ppis.rb
 }
 lib=lib{
  bow=rbbt/bow {
   bow.rb
   dictionary.rb
   misc.rb
  }
  nlp=rbbt/nlp{
   nlp.rb
   open_nlp=open_nlp{
    sentence_splitter.rb
   }
   genia=genia{
    sentence_splitter.rb
   }
  }
  ner=rbbt/ner{
   segment.rb
   segment=segment{
    docid.rb
    named_entity.rb
    token.rb
    segmented.rb
    transformed.rb
    relationship.rb
   }

   NER.rb

   abner.rb
   banner.rb
   g_norm_plus.rb
   
   linnaeus.rb

   oscar3.rb
   oscar4.rb
   chemical_tagger.rb

   regexpNER.rb
   token_trieNER.rb
   ngram_prefix_dictionary.rb
   patterns.rb

   finder.rb

   rnorm.rb
   rnorm=rnorm{
    cue_index.rb
    tokens.rb
   }
  }
  corpus=rbbt/corpus{
   document.rb
   document_repo.rb
   corpus.rb
   sources=sources{
    pubmed.rb
   }
  }
  entity=rbbt/entity{
   document.rb
  }
 }
 share=share{
  software=install/software filter="*"{
   ABNER
   Linnaeus
   BANNER
   GNormPlus
   OpenNLP
   OSCAR3
   OSCAR4
   ChemicalTagger
   StanfordParser
   Geniass
   Gdep
  }
  rnorm=rnorm filter="*"{
   cue_default
   tokens_default
  }
 }
 test=test filter="*" {
  test_helper.rb
  data=data{
  }
  bow=rbbt/bow{
   test_bow.rb
   test_dictionary.rb
   test_misc.rb
  }
  ner=rbbt/ner{
   test_NER.rb
   test_abner.rb
   test_banner.rb
   test_chemical_tagger.rb
   test_finder.rb
   test_linnaeus.rb
   test_ngram_prefix_dictionary.rb
   test_oscar4.rb
   test_patterns.rb
   test_regexpNER.rb
   test_rnorm.rb
   test_segment.rb
   test_token_trieNER.rb
  }
  annotations=rbbt/annotations{

 }
 data=data{
 }
}
