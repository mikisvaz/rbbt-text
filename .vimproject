rbbt-text=/$PWD filter="*.rb *.rake Rakefile *.rdoc *.R *.sh *.js *.haml *.sass *.txt *.conf" {
 Rakefile
 bin=bin {
  get_ppis.rb
 }
 lib=lib{
  bow=rbbt/bow {
   bow.rb
   dictionary.rb
   misc.rb
  }
  nlp=rbbt/nlp{
   nlp.rb
   genia=genia{
    sentence_splitter.rb
   }
  }
  ner=rbbt/ner{
   segment.rb
   segment=segment{
    named_entity.rb
    token.rb
    segmented.rb
    transformed.rb
    relationship.rb
   }

   NER.rb

   abner.rb
   banner.rb

   oscar3.rb
   oscar4.rb
   chemical_tagger.rb

   regexpNER.rb
   token_trieNER.rb
   ngram_prefix_dictionary.rb
   patterns.rb

   rnorm.rb
   rnorm=rnorm{
    cue_index.rb
    tokens.rb
   }
  }
  corpus=rbbt/corpus{
   document.rb
   document_repo.rb
   corpus.rb
   sources=sources{
    pubmed.rb
   }
  }
 }
 share=share{
  software=install/software filter="*"{
   ABNER
   BANNER
   OSCAR3
   OSCAR4
   ChemicalTagger
   StanfordParser
   Geniass
   Gdep
  }
  rnorm=rnorm filter="*"{
   cue_default
   tokens_default
  }
 }
 test=test {
  test_helper.rb
  data=data{
  }
  bow=rbbt/bow{
   test_bow.rb
   test_misc.rb
   test_dictionary.rb
  }
  ner=rbbt/ner{
   test_abner.rb
   test_banner.rb
   test_named_entity.rb
   test_oscar3.rb
   test_polysearch.rb
   test_regexpNER.rb
   test_tokenNER.rb
   test_rnorm.rb
  }
  annotations=rbbt/annotations{
   test_annotation_repo.rb
   test_corpus.rb
   test_document_repo.rb
 }
 data=data{
 }
}
