rbbt-text=/$PWD filter="*.rb *.rake Rakefile *.rdoc *.R *.sh *.js *.haml *.sass *.txt *.conf" {
 Rakefile
 bin=bin {
  get_ppis.rb
 }
 lib=lib{

  rbbt=rbbt{
   document.rb
   document=document{
    corpus.rb
    corpus=corpus{
      pubmed.rb
    }
    annotation.rb
   }
   segment.rb
   segment=segment{
    range_index.rb
    annotation.rb
    named_entity.rb
    token.rb

    overlaps.rb
    segmented.rb

    transformed.rb

    encoding.rb

    tsv.rb
   }

   // OLD STUFF

   text=.save/text{
    document.rb
    corpus.rb
    corpus=corpus{
     document.rb
     document_repo.rb
     sources=sources{
      pmid.rb
     }
    }
    segment.rb
    segment=segment{
     docid.rb
     named_entity.rb
     relationship.rb
     segmented.rb
     token.rb
     transformed.rb
     encoding.rb

    }
   }
  }

  bow=rbbt/bow {
   bow.rb
   dictionary.rb
   misc.rb
  }
  nlp=rbbt/nlp{
   nlp.rb
   open_nlp=open_nlp{
    sentence_splitter.rb
   }
   genia=genia{
    sentence_splitter.rb
   }
  }
  ner=rbbt/ner{
   NER.rb
   abner.rb
   banner.rb
   brat.rb
   chemical_tagger.rb
   finder.rb
   g_norm_plus.rb
   linnaeus.rb
   ngram_prefix_dictionary.rb
   oscar3.rb
   oscar4.rb
   patterns.rb
   regexpNER.rb
   token_trieNER.rb
   rnorm.rb
   rnorm=rnorm{
    cue_index.rb
    tokens.rb
   }
  }
 }

 share=share{
  software=install/software filter="*"{
   ABNER
   Linnaeus
   BANNER
   GNormPlus
   OpenNLP
   OSCAR3
   OSCAR4
   ChemicalTagger
   StanfordParser
   Geniass
   Gdep
  }
  rnorm=rnorm filter="*"{
   cue_default
   tokens_default
  }
 }

 test=test filter="*" {
  test_helper.rb
  bow=rbbt/bow{
   test_bow.rb
   test_dictionary.rb
   test_misc.rb
  }
  text=rbbt/text{
   test_corpus.rb
   test_document.rb
   test_segment.rb
   segment=segment{
    test_named_entity.rb
    test_relationship.rb
    test_segmented.rb
    test_transformed.rb
    }
  }
  ner=rbbt/ner{
   test_NER.rb
   test_abner.rb
   test_banner.rb
   test_brat.rb
   test_chemical_tagger.rb
   test_finder.rb
   test_g_norm_plus.rb
   test_linnaeus.rb
   test_ngram_prefix_dictionary.rb
   test_oscar4.rb
   test_patterns.rb
   test_regexpNER.rb
   test_rnorm.rb
   test_token_trieNER.rb
  }
  annotations=rbbt/annotations{
 }
 data=data{
 }
}
